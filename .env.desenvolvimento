DB_PASSWORD=root
DB_NAME=MarketMeet

# Configurações de Segurança (OBRIGATÓRIO - Gere uma chave segura!)
JWT_SECRET=sua-chave-secreta-super-forte-aqui-no-minimo-32-caracteres-aleatorios

# Configurações de Rate Limit
RATE_LIMIT_WINDOW_MS=900000  # 15 minutos
RATE_LIMIT_MAX=100  # requisições por janela

# Configurações de CORS
CORS_ORIGIN=http://localhost:4200
